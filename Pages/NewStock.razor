@page "/newstock"

<PageTitle>MyBlazorBroker - Neue Aktie hinzufügen</PageTitle>

<div id="newStock" class="row">
    <div class="input-field col s8 m6">
        <input id="name" type="text" class="validate" @bind="StockName">
        <label class="active" for="name">Name</label>
    </div>
    <div class="col s4">
        <button id="addStock" class="btn waves-effect waves-light blue darken-1"
                style="margin-top: 20px;" @onclick="AddStock">OK</button>
    </div>
</div>